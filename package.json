{
  "name": "construction-timesheet-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=512' next dev",
    "dev:stable": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=512 --no-warnings' next dev --experimental-https=false",
    "build": "npx prisma generate && next build",
    "build:db": "npx prisma generate && npx prisma db push",
    "build:production": "npm install && npm run build:db && npm run db:seed && next build",
    "vercel-build": "npx prisma generate && npx prisma db push && npm run db:seed && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "pages:deploy": "npx wrangler pages deploy .next"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.1",
    "@tanstack/react-query": "^5.17.9",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.0.6",
    "file-saver": "^2.0.5",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
